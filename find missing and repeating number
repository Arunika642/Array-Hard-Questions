brute

#include<bits/stdc++.h>

vector<int> findMissingRepeatingNumbers(vector < int > a) {
    // Write your code here
    vector<int> ans;
    map<int,int> mpp;
    for(int i=0;i<a.size();i++)
    {
        mpp[a[i]]++;
    }

    for(auto it:mpp)
    {
        if(it.second==2)
        {
            ans.push_back(it.first);
            break;
        }
    }
    for(int i=1;i<=a.size();i++)
    {
        if(mpp.find(i)==mpp.end())
        {
            ans.push_back(i);
            break;
        }
    }
    return ans;
}
